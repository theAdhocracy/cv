<script>
	// Function: Set theme
	const SetTheme = (theme) => {
		// Set data attribute for styles
		themeRoot.setAttribute("data-theme", theme);

		// Record user preference
		localStorage.setItem("theme", theme);
	};

	// Function: Show theme menu
	const ShowThemeMenu = (state) => {
		// Show/hide options
		themeMenu.toggleAttribute("hidden");

		if (state === "show") {
			themeToggle.setAttribute("aria-expanded", "true");
			themeToggle.setAttribute("aria-label", "Close theme options");
		} else {
			themeToggle.setAttribute("aria-expanded", "false");
			themeToggle.setAttribute("aria-label", "Open theme options");
		}
	};

	// Theme toggle (dark/light mode)
	const themeToggle = document.querySelector("#theme-switcher > button");
	const themeMenu = document.querySelector("#theme-options");
	const themeOptions = document.querySelectorAll("#theme-switcher ul button");
	const themeRoot = document.querySelector("html");

	// Open/close theme menu
	themeToggle.addEventListener("click", () => {
		if (themeToggle.getAttribute("aria-expanded") === "true") {
			ShowThemeMenu("hide");
		} else {
			ShowThemeMenu("show");
			// Close menu if click elsewhere on the page
			window.addEventListener(
				"mouseup",
				(event) => {
					if (event.target.closest("#theme-switcher")) {
						return;
					} else {
						ShowThemeMenu("hide");
					}
				},
				{ once: true }
			);
		}
	});

	// Retrieve existing theme preference
	let theme = localStorage.getItem("theme") || "system";

	// Add event to each theme option
	themeOptions.forEach((option) => {
		option.addEventListener("click", () => {
			// Update theme (if present)
			if (option.dataset.theme) {
				theme = option.dataset.theme;
			}

			SetTheme(theme);

			// Close menu if click elsewhere on the page
			window.addEventListener(
				"mouseup",
				(event) => {
					if (event.target.closest("#theme-switcher")) {
						return;
					} else {
						ShowThemeMenu("hide");
					}
				},
				{ once: true }
			);
		});
	});
</script>

